{
  "name": "@peripleo/peripleo",
  "version": "0.8.8",
  "description": "React components for map-based search interfaces",
  "author": "Rainer Simon",
  "license": "BSD-3-Clause",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/peripleo/peripleo.git"
  },
  "bugs": {
    "url": "https://github.com/peripleo/peripleo/issues"
  },
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "preview": "vite preview"
  },
  "main": "./dist/peripleo.es.js",
  "module": "./dist/peripleo.es.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "default-theme"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/peripleo.es.js",
      "require": "./dist/peripleo.cjs.js"
    },
    "./peripleo.css": "./dist/peripleo.css",
    "./default-theme": "./default-theme/index.ts"
  },
  "typesVersions": {
    "*": {
      "default-theme": [
        "./default-theme/index.ts"
      ]
    }
  },
  "devDependencies": {
    "@types/chroma-js": "^3.1.2",
    "@types/react": "^18.3.27",
    "@types/react-dom": "^18.3.7",
    "@types/react-resizable": "^3.0.8",
    "@vitejs/plugin-react": "^4.7.0",
    "vite": "^6.4.1",
    "vite-plugin-dts": "^4.5.4",
    "vite-tsconfig-paths": "^5.1.4"
  },
  "peerDependencies": {
    "react": ">=17.x || >=18.x",
    "react-dom": ">=17.x || >=18.x"
  }
}
